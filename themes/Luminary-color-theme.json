      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#3BADC1"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#3BADC1"
      }
    },
    {
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "italic"
