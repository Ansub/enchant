      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#FF293B"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#3BADC1"
      }
    }
